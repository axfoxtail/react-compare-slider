    "test:watch": "yarn test --coverage --watch",
    "lint": "tsdx lint",
    "lint-staged": "lint-staged",
    "storybook": "start-storybook -p 6006 --ci",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "np": {
    "yarn": true
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "pretty-quick",
      "eslint --fix",
      "git add",
      "yarn test --bail --findRelatedTests"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
