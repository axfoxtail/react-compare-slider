{
  "name": "react-compare-slider",
  "version": "2.2.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nerdyman/react-compare-slider.git"
  },
  "homepage": "https://react-compare-slider.vercel.app",
  "author": {
    "email": "averynerdyman@gmail.com",
    "name": "nerdyman",
    "url": "https://github.com/nerdyman"
  },
  "description": "A slider component to compare any two React components in landscape or portrait orientation. It supports custom images, videos... and everything else.",
  "keywords": [
    "react",
    "slider",
    "comparison",
    "compare",
    "image",
    "video",
    "component",
    "image comparison",
    "twentytwenty",
    "portrait",
    "typescript"
  ],
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/react-compare-slider.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "NODE_ENV=production tsdx build",
    "dedupe-types": "rm -rf node_modules && yarn remove @types/react && yarn && yarn add --dev @types/react && npx yarn-deduplicate --packages @types/react yarn.lock",
    "release": "yarn build && np --no-2fa",
    "test": "test-storybook",
    "test:coverage": "yarn test --coverage",
    "lint": "tsdx lint",
    "lint-staged": "lint-staged",
    "storybook": "start-storybook -p 6006 --ci",
    "storybook:build": "build-storybook"
  },
  "browserslist": {
    "production": [
      "last 2 chrome versions",
      "last 2 edge versions",
      "last 2 firefox versions",
      "last 2 safari versions",
      "not dead",
      "not ie > 0",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "pretty-quick --staged",
      "eslint --fix",
      "yarn test --bail --findRelatedTests"
    ]
  },
  "np": {
    "yarn": true
  },
  "prettier": {
    "printWidth": 90,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "devDependencies": {
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-coverage": "^0.0.4",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/addon-storysource": "^6.5.10",
    "@storybook/jest": "^0.0.10",
    "@storybook/preset-typescript": "3.0.0",
    "@storybook/react": "^6.5.10",
    "@storybook/test-runner": "^0.7.1",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.10",
    "@types/node": "^14.14.17",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "google-maps-react": "^2.0.6",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "np": "^7.2.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "remark-codesandbox": "^0.10.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {}
}
