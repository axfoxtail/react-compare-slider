    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@storybook/addon-actions": "^5.3.0-alpha.34",
    "@storybook/addon-docs": "^5.3.0-alpha.34",
    "@storybook/addon-info": "^5.3.0-alpha.34",
