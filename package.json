    "@typescript-eslint/parser": "^2.3.2",
    "babel-loader": "^8.0.6",
    "eslint": "^6.5.1",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.1",
    "np": "^5.1.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react": "^16.10.2",
    "react-docgen-typescript-loader": "^3.3.0",
    "react-dom": "^16.10.2",
    "ts-loader": "^6.2.1",
